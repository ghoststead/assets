{
    "name": "assets",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "eslint-plugin-jest": "^24.1.7",
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@octokit/core": "^3.2.5",
        "@octokit/rest": "^18.3.4",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "babel-eslint": "^10.1.0",
        "bootstrap": "^5.0.0-beta2",
        "dotenv": "^8.2.0",
        "dotenv-webpack": "^7.0.1",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^7.0.0-0",
        "jest": "^26.6.3",
        "mime": "^2.5.2",
        "sass": "^1.32.8",
        "sass-loader": "^10.1.1",
        "vue-router": "^4.0.4",
        "vuex": "^4.0.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-recommended",
            "eslint:recommended",
            "plugin:jest/all"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "indent": [
                "error",
                4
            ],
            "vue/html-indent": [
                "error",
                4
            ],
            "quotes": [
                "error",
                "single",
                {
                    "avoidEscape": true
                }
            ],
            "semi": [
                "error",
                "always"
            ],
            "linebreak-style": [
                "error",
                "unix"
            ],
            "prefer-template": "error"
        },
        "globals": {
            "jest/globals": true
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
